{
    "plugins": ["jsdoc-export-default-interop"],
    "recurseDepth": 10,
    "opts": {
      "recurse": true,
      "destination": "./docs/"
    },
    "source": {
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "node_modules|dist"
    },
    "sourceType": "module",
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc", "closure"]
    },
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
    }
  }